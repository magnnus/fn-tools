{
  "name": "@autots/cookie",
  "version": "1.0.0",
  "description": "some functions to manipulate cookies in browser",
  "source": "src/index.ts",
  "browser": "dist/index.js",
  "types": "dist/index.d.ts",
  "browserslist": "> 0.5%, last 2 versions, not dead",
  "scripts": {
    "build": "parcel build --no-source-maps",
    "postbuild": "npm run doc",
    "check": "tsc src --noEmit",
    "doc": "documentation build src/index.ts --parse-extension ts -f md -o README.md --github --document-exported"
  },
  "targets": {
    "browser": {
      "isLibrary": true
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist/index.js",
    "dist/index.d.ts",
    "src"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/magnnus/fn-tools.git",
    "directory": "packages/cookie"
  },
  "keywords": [
    "cookie",
    "typescript"
  ],
  "author": "magnnus",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/magnnus/fn-tools/issues"
  },
  "homepage": "https://github.com/magnnus/fn-tools#readme",
  "devDependencies": {
    "@parcel/packager-ts": "~2.0.1",
    "@parcel/transformer-typescript-types": "~2.0.1",
    "documentation": "~13.2.5",
    "parcel": "^2.0.1",
    "typescript": "~4.5.2"
  }
}
